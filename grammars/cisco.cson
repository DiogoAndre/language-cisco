  'banner-timeout':
    'patterns': [
      {
        'begin': '^(banner prompt-timeout \\^.*)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
          '2':
            'name': 'comment.banner.cisco'
        'end': '^(.*\\^)'
        'endCaptures':
          '1':
            'name': 'comment.banner.cisco'
        'name': 'keyword.other.service'
        'patterns': [
          {
            'match': '^.*$'
            'name': 'comment.banner.cisco'
          }
        ]
      }
    ]
  'banner-asa':
    'patterns': [
      {
        'begin': '^(banner motd)(.[^\\^]*)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
          '2':
            'name': 'comment.banner.cisco'
        'end': '$'
        'endCaptures':
          '1':
            'name': 'comment.banner.cisco'
        'name': 'keyword.other.service'
        'patterns': [
          {
            'match': '^.*$'
            'name': 'comment.banner.cisco'
          }
        ]
      }
    ]
  'buffers':
    'patterns': [
      {
        'begin': '^(buffers)\\s'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
        'end': '$'
        'patterns': [
          {
            'match': '(small|medium|large|permanent|max-free|min-free|initial)\\s'
            'name': 'keyword.other.buffers'
          }
        ]
      }
    ]
  'clock':
    'patterns': [
      {
        'begin': '^(clock)\\s(timezone|summer-time)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
          '2':
            'name': 'keyword.other.clock'
        'end': '$'
        'patterns': [
          {
            'match': '(recurring)\\s'
            'name': 'keyword.other.clock'
          }
        ]
      }
    ]
  'crypto_dynmap':
    'patterns': [
      {
        'begin': '^(crypto dynamic-map)(?=\\s)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
        'end': '^(!)'
        'endCaptures':
          '1':
            'name': 'comment.documentation.cisco'
        'patterns': [
          {
            'match': '\\s(set|transform-set)(?=\\s)'
            'name': 'keyword.other.crypto'
          }
          {
            'include': '#interface_types'
          }
          {
            'include': '#ip_address'
          }
        ]
      }
    ]
  'crypto_ike':
    'patterns': [
      {
        'begin': '^(crypto isakmp)(?=\\s)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
        'end': '($)'
        'endCaptures':
          '1':
            'name': 'comment.documentation.cisco'
          '2':
            'name': 'constant.language.cisco'
        'patterns': [
          {
            'match': '\\s(encr|encryption|hash|authentication|group|lifetime|policy|dns|key|pool|acl|domain|configuration|client|group)(?=\\s)'
            'name': 'keyword.other.crypto'
          }
          {
            'include': '#disable'
          }
          {
            'include': '#ip_address'
          }
          {
            'include': '#description'
          }
        ]
      }
    ]
  'crypto_ike_policy':
    'patterns': [
      {
        'begin': '^(crypto isakmp policy|crypto ikev2 policy) (\\d*)$'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
          '2':
            'name': 'constant.numeric.cisco'
        'end': '(^!|^exit)|(crypto isakmp)|(crypto ikev2 policy)'
        'endCaptures':
          1:
            name: 'comment.documentation.cisco'
          2:
            name: 'constant.language.cisco'
          3:
            name: 'constant.language.cisco'
        'patterns': [
          {
            'match': '(encr|prf|integrity|encryption|hash|authentication|group|lifetime|policy|dns|key|pool|acl|domain|configuration|client|group)(?=\\s)'
            'name': 'keyword.other.crypto'
          }
          {
            include: '#disable'
          }
          {
            include: '#numeric'
          }
          {
            include: '#ip_address'
          }
          {
            include: '#description'
          }
        ]
      }
    ]
  'crypto_ipsec':
    'patterns': [
      {
        'begin': '^(crypto ipsec)(?=\\s)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
        'end': '$'
        'patterns': [
          {
            'match': '\\s(client|ezvpn|ezvpnclient|security-association|lifetime|seconds|transform-set)(?=\\s)'
            'name': 'keyword.other.crypto'
          }
          {
            'include': '#interface_types'
          }
          {
            'include': '#ip_address'
          }
        ]
      }
    ]
  'crypto_ca':
    'patterns': [
      {
        'begin': '^(crypto ca)(?=\\s)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
        'end': '($)'
        'endCaptures':
          '1':
            'name': 'comment.documentation.cisco'
        'patterns': [
          {
            'match': '\\s(certificate|trustpoint|chain)(?=\\s)'
            'name': 'keyword.other.crypto'
          }
          {
            'match': '\\s(.*)(?=\\s)'
            'name': 'string'
          }
        ]
      }
    ]
  'certificate':
    'patterns': [
      {
        'begin': '^\\s(certificate)(?=\\s)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
        'end': '.*(quit)'
        'endCaptures':
          '1':
            'name': 'comment.documentation.cisco'
        'patterns': [
          {
            'match': '\\s(ca)(?=\\s)'
            'name': 'keyword.other.crypto'
          }
          {
            'match': '\\s(.*)(?=\\s)'
            'name': 'string'
          }
        ]
      }
    ]
  'crypto_map':
    'patterns': [
      {
        'begin': '^(crypto map)(?=\\s)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
        'end': '$'
        'patterns': [
          {
            'match': '\\s(static-map|local-address|client|authentication|configuration|isakmp|authorization|ipsec-isakmp|dynamic|address|list|match)(?=\\s)'
            'name': 'keyword.other.crypto'
          }
          {
            'include': '#interface_types'
          }
          {
            'include': '#ip_address'
          }
        ]
      }
    ]
  'crypto_pki':
    'patterns': [
      {
        'begin': '^(crypto pki)(?=\\s)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
        'end': '(!|quit)'
        'endCaptures':
          '1':
            'name': 'comment.documentation.cisco'
        'patterns': [
          {
            'match': '\\s(certificate|trustpoint|chain|self(-)?signed|enrollment|revocation-check|rsakeypair|subject-name)(?=\\s)'
            'name': 'keyword.other.crypto'
          }
          {
            'include': '#disable'
          }
          {
            'include': '#ip_address'
          }
          {
            'include': '#description'
          }
        ]
      }
    ]
  'description':
    'patterns': [
      {
        'begin': '^(description|!|remark)(?=\\s)'
        'beginCaptures':
          '1':
            'name': 'comment.documentation.cisco'
        'end': '$'
        'patterns': [
          {
            'match': '(.*)'
            'name': 'comment.documentation.cisco'
          }
        ]
      }
    ]
  'dhcp_pool':
    'patterns': [
      {
        'begin': '^(ip )(dhcp pool)\\s'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
          '2':
            'name': 'keyword.other.dhcp'
        'end': '^(!)'
        'endCaptures':
          '1':
            'name': 'comment.documentation.cisco'
        'patterns': [
          {
            'match': '(network|dns-server|default-router|option|ip|lease)\\s'
