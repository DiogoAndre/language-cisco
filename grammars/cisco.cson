'repository':
  'aaa':
    'patterns': [
      {
        'begin': '^(no )?(aaa)\\s'
        'beginCaptures':
          '1':
            'name': 'keyword.negative.cisco'
          '2':
            'name': 'constant.language.cisco'
        'end': '$'
        'patterns': [
          {
            'match': '(new-model|authentication|authorization|accounting|login|exec|commands|group|local|session-id|server|policy)\\s'
            'name': 'keyword.other.aaa'
          }
        ]
      }
    ]
  'access_list':
    'patterns': [
      {
        'begin': '^(access-list) (\\w*)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
          '2':
            'name': 'constant.numeric.cisco'
        'end': '($)'
        'patterns': [
          {
            'match': '(extended|compiled|udp|esp|tcp|icmp|ip|eq|range)\\s'
            'name': 'keyword.other.acl'
          }
          {
            'match': '(permit)\\s'
            'name': 'string'
          }
          {
            'match': '(host) (\\S*)'
            captures:
              1:
                'name': 'keyword.other.acl'
              2:
                'name': 'constant.numeric.cisco'
          }
          {
            'match': '(deny)\\s'
            'name': 'entity.other.inherited-class'
          }
          {
            'match': '(any)'
            'name': 'entity.other.inherited-class'
          }
          {
            'include': '#ipv6_address'
          }
          {
            'include': '#ip_address'
          }
          {
            'include': '#subnet_mask'
          }
          {
            'include': '#description'
          }
        ]
      }
    ]
  'archive':
    'patterns': [
      {
        'begin': '^(archive)(?=\\s)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
        'end': '^(!)'
        'endCaptures':
          '1':
            'name': 'comment.documentation.cisco'
        'patterns': [
          {
            'match': '\\b(path|maximum|notify|(write-)?memory|time-period|reserve|console|logging|size|notify|syslog|contenttype|log|config|hidekeys)(?=\\s)'
            'name': 'keyword.other.archive'
          }
        ]
      }
    ]
  'banner-motd':
    'patterns': [
      {
        'begin': '^(banner motd \\^.*)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
          '2':
            'name': 'comment.banner.cisco'
        'end': '^(.*\\^)'
        'endCaptures':
          '1':
            'name': 'comment.banner.cisco'
        'name': 'keyword.other.service'
        'patterns': [
          {
            'match': '^.*$'
            'name': 'comment.banner.cisco'
          }
        ]
      }
    ]
  'banner-login':
    'patterns': [
      {
        'begin': '^(banner login \\^.*)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
          '2':
            'name': 'comment.banner.cisco'
        'end': '^(.*\\^)'
        'endCaptures':
          '1':
            'name': 'comment.banner.cisco'
        'name': 'keyword.other.service'
        'patterns': [
          {
            'match': '^.*$'
            'name': 'comment.banner.cisco'
          }
        ]
      }
    ]
  'banner-exec':
    'patterns': [
      {
        'begin': '^(banner exec \\^.*)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
          '2':
            'name': 'comment.banner.cisco'
        'end': '^(.*\\^)'
        'endCaptures':
          '1':
            'name': 'comment.banner.cisco'
        'name': 'keyword.other.service'
        'patterns': [
          {
            'match': '^.*$'
            'name': 'comment.banner.cisco'
          }
        ]
      }
